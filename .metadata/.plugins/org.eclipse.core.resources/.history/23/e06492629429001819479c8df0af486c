
public class FamilyDetails {

	public static void main(String[] args) {
		Family sayed = new Family("Sayed", "Siddeque","Male", 47);
		Family tahir = new Family("Tahir", "Siddeque", "Male",22);
		Family mahmuda = new Family("Mahmudfa","Begum", "Female", 27);
		
		sayed.displayFamilyDetails();
		tahir.displayFamilyDetails();
		mahmuda.displayFamilyDetails();
		
	}
}
